#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ramongiovani/Documents/GitHub/trybe-exercicios/front-end/secao-01-implementando-aplicacao/dia-02-assincrono-promises-fetch/project-dog-cat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ramongiovani/Documents/GitHub/trybe-exercicios/front-end/secao-01-implementando-aplicacao/dia-02-assincrono-promises-fetch/project-dog-cat/node_modules/.pnpm/node_modules"
fi
"$basedir/../../../../../esbuild@0.14.54/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
